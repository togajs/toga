{
  "name": "toga",
  "version": "0.5.0",
  "description": "The streaming documentation generator.",
  "keywords": [
    "toga",
    "generator",
    "api",
    "documentation",
    "docblock",
    "docs",
    "reference",
    "styleguide"
  ],
  "homepage": "https://togajs.com",
  "bugs": "https://github.com/togajs/toga/issues",
  "license": "MIT",
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "main": "dist/toga.js",
  "bin": {
    "toga": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/togajs/toga.git"
  },
  "dependencies": {
    "interpret": "^0.5.2",
    "liftoff": "^2.0.3",
    "multistream": "^1.4.3",
    "vinyl-fs": "^1.0.0",
    "yargs": "^3.8.0"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "coveralls": "^2.11.2",
    "eslint": "^0.20.0",
    "expect": "^1.6.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.4",
    "nodemon": "^1.3.7",
    "stream-array": "^1.1.0",
    "toga-css": "^0.1.3",
    "toga-js": "^0.1.3",
    "toga-markdown": "^0.1.2",
    "toga-sample": "*"
  },
  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "build": "babel src --out-dir dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src/*.js test/*.js",
    "test": "babel-node ./node_modules/.bin/isparta cover --include src/*.js ./node_modules/.bin/_mocha -- test/*.js",
    "watch": "nodemon --exec 'npm run test' --watch src --watch test"
  }
}
