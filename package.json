{
  "name": "toga",
  "version": "0.5.1",
  "description": "The streaming documentation generator.",
  "keywords": [
    "toga",
    "generator",
    "documentation",
    "docblock",
    "docs",
    "reference",
    "styleguide"
  ],
  "homepage": "https://togajs.com",
  "bugs": "https://github.com/togajs/toga/issues",
  "license": "MIT",
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "main": "dist/toga.js",
  "bin": {
    "toga": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/togajs/toga.git"
  },
  "dependencies": {
    "duplexify": "^3.4.2",
    "find-config": "^0.3.0",
    "gulp-plumber": "^1.0.1",
    "merge-stream": "^1.0.0",
    "through2": "^2.0.0",
    "vinyl-fs": "^1.0.0",
    "yargs": "^3.24.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-istanbul": "^0.3.19",
    "coveralls": "^2.11.4",
    "eslint": "^1.3.1",
    "expect": "^1.9.0",
    "mocha": "^2.3.0",
    "mtil": "^0.1.3",
    "nodemon": "^1.4.1",
    "stream-array": "^1.1.1"
  },
  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "build": "babel src --out-dir dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src/*.js test/*.js",
    "test": "babel-node node_modules/.bin/babel-istanbul cover --report text --report lcov --root src node_modules/.bin/_mocha -- 'test/*.js'",
    "watch": "nodemon --exec 'npm run test' --watch src --watch test"
  }
}
